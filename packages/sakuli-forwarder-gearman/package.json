{
  "name": "@sakuli/forwarder-gearman",
  "version": "2.4.0",
  "description": "Gearman forwarder for Sakuli v2+",
  "keywords": [
    "monitoring",
    "e2e",
    "sakuliPreset"
  ],
  "author": "Simon Hofmann <simon.hofmann@consol.de>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@nut-tree/secrets": "4.0.2",
    "@sakuli/commons": "2.4.0",
    "@sakuli/core": "2.4.0",
    "@sakuli/result-builder-commons": "2.4.0",
    "@sakuli/result-builder-omd": "2.4.0",
    "gearman": "2.1.0",
    "sneer": "1.0.1",
    "common-tags": "1.8.0"
  },
  "devDependencies": {
    "@types/common-tags": "1.8.0",
    "@types/jest": "26.0.15",
    "@types/luxon": "1.25.0",
    "@types/node": "14.14.10",
    "javascript-obfuscator": "2.9.4",
    "jest": "26.6.3",
    "ts-jest": "26.4.4",
    "typescript": "3.8.3"
  },
  "scripts": {
    "obfuscate": "javascript-obfuscator ./dist --output ./out --config ../../scripts/obfuscate.js",
    "copy": "node ../../scripts/move-files.js ./out .",
    "build": "tsc",
    "build:release": "tsc && npm run obfuscate && npm run copy",
    "test": "jest",
    "prepublishOnly": "npm run build:release && npm test"
  }
}
