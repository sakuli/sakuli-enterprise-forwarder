{
  "name": "@sakuli/forwarder-gearman",
  "version": "2.2.0",
  "description": "Gearman forwarder for Sakuli v2+",
  "keywords": [
    "monitoring",
    "e2e"
  ],
  "author": "Simon Hofmann <simon.hofmann@consol.de>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@nut-tree/secrets": "4.0.0",
    "@sakuli/commons": "2.2.0",
    "@sakuli/core": "2.2.0",
    "@sakuli/result-builder-commons": "^2.2.0",
    "@sakuli/result-builder-omd": "^2.2.0",
    "gearman": "2.1.0",
    "sneer": "1.0.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.17",
    "@types/luxon": "1.15.2",
    "@types/node": "12.7.1",
    "javascript-obfuscator": "0.18.1",
    "jest": "24.5.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3"
  },
  "scripts": {
    "obfuscate": "javascript-obfuscator ./dist --output ./out --config ../../scripts/obfuscate.js",
    "copy": "node ../../scripts/move-files.js ./out .",
    "build": "tsc",
    "build:release": "tsc && npm run obfuscate && npm run copy",
    "test": "jest",
    "prepublishOnly": "lerna bootstrap --ignore integration-tests && npm run build:release && npm test"
  }
}
