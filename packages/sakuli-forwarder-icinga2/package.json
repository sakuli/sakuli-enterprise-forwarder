{
  "name": "@sakuli/forwarder-icinga2",
  "version": "2.2.0",
  "description": "Inciga2 forwarder for Sakuli v2+",
  "keywords": [
    "monitoring",
    "e2e"
  ],
  "author": "Tim Keiner <keiner.tim@gmail.com>",
  "main": "dist/index.js",
  "dependencies": {
    "@sakuli/commons": "2.2.0",
    "@sakuli/core": "2.2.0",
    "@sakuli/result-builder-commons": "2.2.0",
    "axios": "0.19.0",
    "class-validator": "0.9.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.17",
    "@types/node": "12.6.9",
    "@types/twig": "1.12.2",
    "javascript-obfuscator": "0.18.1",
    "jest": "24.5.0",
    "sneer": "1.0.1",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3"
  },
  "scripts": {
    "obfuscate": "javascript-obfuscator ./dist --output ./out --config ../../scripts/obfuscate.js",
    "copy": "node ../../scripts/move-files.js ./out .",
    "build": "tsc",
    "build:release": "tsc && npm run obfuscate && npm run copy",
    "test": "jest",
    "prepublishOnly": "lerna bootstrap --ignore integration-tests && npm run build:release && npm test"
  }
}
