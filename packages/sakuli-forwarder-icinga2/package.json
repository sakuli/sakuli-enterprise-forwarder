{
  "name": "@sakuli/forwarder-icinga2",
  "version": "2.5.0",
  "description": "Inciga2 forwarder for Sakuli v2+",
  "keywords": [
    "monitoring",
    "e2e",
    "sakuliPreset"
  ],
  "author": "Tim Keiner <keiner.tim@gmail.com>",
  "main": "dist/index.js",
  "dependencies": {
    "@sakuli/commons": "2.5.0",
    "@sakuli/core": "2.5.0",
    "@sakuli/result-builder-commons": "2.5.0",
    "axios": "0.21.1",
    "class-validator": "0.12.2",
    "common-tags": "1.8.0"
  },
  "devDependencies": {
    "@types/common-tags": "1.8.0",
    "@types/jest": "27.0.1",
    "@types/node": "14.14.10",
    "javascript-obfuscator": "2.9.4",
    "jest": "27.2.0",
    "sneer": "1.0.1",
    "ts-jest": "27.0.5",
    "typescript": "4.1.3"
  },
  "scripts": {
    "obfuscate": "javascript-obfuscator ./dist --output ./out --config ../../scripts/obfuscate.js",
    "copy": "node ../../scripts/move-files.js ./out ./dist",
    "build": "tsc",
    "build:release": "tsc && npm run obfuscate && npm run copy",
    "test": "jest",
    "prepublishOnly": "npm run build:release && npm test"
  }
}