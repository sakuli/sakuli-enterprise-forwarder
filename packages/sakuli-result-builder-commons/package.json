{
  "name": "@sakuli/result-builder-commons",
  "version": "2.3.0",
  "description": "Common code for result builders",
  "author": "Simon Hofmann <simon.hofmann@consol.de>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "obfuscate": "javascript-obfuscator ./dist --output ./out --config ../../scripts/obfuscate.js",
    "copy": "node ../../scripts/move-files.js ./out .",
    "build": "tsc",
    "build:release": "tsc && npm run obfuscate && npm run copy",
    "prepublishOnly": "npm run build:release && npm test"
  },
  "dependencies": {
    "@sakuli/commons": "2.3.1-next.62",
    "@sakuli/core": "2.3.1-next.62",
    "class-validator": "0.9.1",
    "common-tags": "1.8.0",
    "dateformat": "3.0.3",
    "sneer": "1.0.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/common-tags": "1.8.0",
    "@types/dateformat": "3.0.0",
    "@types/jest": "25.2.1",
    "@types/uuid": "3.4.5",
    "@types/yargs": "12.0.12",
    "javascript-obfuscator": "0.18.1",
    "jest": "25.2.7",
    "ts-jest": "25.3.1",
    "typescript": "3.8.3"
  }
}