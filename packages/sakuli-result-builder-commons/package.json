{
  "name": "@sakuli/result-builder-commons",
  "version": "2.4.0",
  "description": "Common code for result builders",
  "author": "Simon Hofmann <simon.hofmann@consol.de>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "obfuscate": "javascript-obfuscator ./dist --output ./out --config ../../scripts/obfuscate.js",
    "copy": "node ../../scripts/move-files.js ./out ./dist",
    "build": "tsc",
    "build:release": "tsc && npm run obfuscate && npm run copy",
    "prepublishOnly": "npm run build:release && npm test"
  },
  "dependencies": {
    "@sakuli/commons": "2.4.0",
    "@sakuli/core": "2.4.0",
    "class-validator": "0.12.2",
    "common-tags": "1.8.0",
    "dateformat": "4.0.0",
    "sneer": "1.0.1",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@types/common-tags": "1.8.0",
    "@types/dateformat": "3.0.1",
    "@types/jest": "26.0.15",
    "@types/uuid": "8.3.0",
    "@types/node": "14.14.10",
    "javascript-obfuscator": "2.9.4",
    "jest": "26.6.3",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  }
}