{
	"name": "@sakuli/forwarder-checkmk",
	"version": "2.0.1",
	"description": "check_mk forwarder for Sakuli v2+",
	"keywords": [
		"monitoring",
		"e2e"
	],
	"author": "Tim Keiner <keiner.tim@gmail.com>",
	"license": "ISC",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"scripts": {
		"obfuscate": "javascript-obfuscator ./dist --output ./out --config ../../scripts/obfuscate.js",
		"copy": "node ../../scripts/move-files.js ./out .",
		"build": "tsc",
		"build:release": "tsc && npm run obfuscate && npm run copy",
		"test": "jest"
	},
	"dependencies": {
		"@sakuli/commons": "next",
		"@sakuli/core": "next",
		"@sakuli/result-builder-commons": "2.0.1",
		"@sakuli/result-builder-checkmk": "2.0.1",
		"axios": "0.19.0",
		"class-validator": "0.9.1",
		"common-tags": "1.8.0"
	},
	"devDependencies": {
		"@types/common-tags": "1.8.0",
		"@types/jest": "24.0.17",
		"@types/node": "12.7.1",
		"javascript-obfuscator": "0.18.1",
		"jest": "24.8.0",
		"sneer": "1.0.1",
		"ts-jest": "24.0.2",
		"typescript": "3.5.3"
	}
}
